%h1 New match

= form_for @match do |f|
  = f.label :home_team_id
  = f.number_field :home_team_id
  %br/

  = f.label :away_team_id
  = f.number_field :away_team_id
  %br/

  = f.label :game_week
  = f.number_field :game_week
  %br/

  = f.label :starts_at
  = f.text_field :starts_at
  %br/

  = f.label :ends_at
  = f.text_field :ends_at
  %br/
  = f.submit 'Create'
